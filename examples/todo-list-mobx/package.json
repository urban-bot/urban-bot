{
    "name": "urban-bot-example-todo-list-mobx",
    "version": "0.0.9",
    "description": "urban bot todo-list-mobx example",
    "repository": {
        "type": "git",
        "url": "https://github.com/urban-bot/urban-bot.git",
        "directory": "examples/todo-list-mobx"
    },
    "license": "MIT",
    "main": "dist/index.js",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "webpack",
        "dev": "nodemon",
        "lint": "eslint \"./src\" --ext \".js,.jsx\"",
        "lint:fix": "npm run lint -- --fix",
        "start": "npm run build && node dist/index.js",
        "prepare": "npm run build"
    },
    "dependencies": {
        "@babel/core": "^7.10.2",
        "@babel/preset-env": "^7.10.2",
        "@babel/preset-react": "^7.10.1",
        "@urban-bot/core": "0.0.9",
        "@urban-bot/facebook": "0.0.9",
        "@urban-bot/slack": "0.0.9",
        "@urban-bot/telegram": "0.0.9",
        "babel-loader": "^8.1.0",
        "dotenv": "^8.2.0",
        "file-loader": "^6.0.0",
        "react": "^16.13.1",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-node-externals": "^1.7.2"
    },
    "devDependencies": {
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jest": "^23.8.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^3.0.0",
        "nodemon": "^2.0.3",
        "prettier": "^2.0.5"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "10"
                    }
                }
            ],
            "@babel/react"
        ]
    },
    "prettier": {
        "semi": true,
        "trailingComma": "all",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 4,
        "arrowParens": "always",
        "endOfLine": "lf"
    },
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "ext": "js,jsx,json",
        "exec": "webpack --mode=development && node ./dist/index.js"
    }
}
